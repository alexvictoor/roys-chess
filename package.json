    "@types/jest": "^26.0.20",
    "assemblyscript": "0.12.1",
    "jest": "^26.6.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.10.1"
  }
}
