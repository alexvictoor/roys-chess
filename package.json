{
  "name": "roys-chess",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "generate-magic": "node ./generate-bishop-magic.js > assembly/fast/generated-bishop-magic.ts && node ./generate-rook-magic.js > assembly/fast/generated-rook-magic.ts",
    "generate-zobrist": "node ./generate-zobrist-keys.js > assembly/fast/generated-zobrist-keys.ts",
    "astest": "asp --verbose",
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --debug",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
